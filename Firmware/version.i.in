	IFND	RPROM_VERSION_I
RPROM_VERSION_I	SET	1

RPROM_VERSION_MAJOR	EQU	@rprom_project_VERSION_MAJOR@
RPROM_VERSION_MINOR	EQU	@rprom_project_VERSION_MINOR@
RPROM_VERSION_PATCH	EQU	@rprom_project_VERSION_PATCH@

RPROM_VERSION	MACRO
	IFGT	RPROM_VERSION_MAJOR
		dc.b	'@rprom_project_VERSION@'
	ELSE
		dc.b	'?.?.?'
	ENDC
	ENDM

	ENDC
